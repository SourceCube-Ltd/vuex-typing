{
  "name": "vuex-typing",
  "description": "Type helpers for vuex.",
  "version": "4.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "d-kimuson",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-kimuson/vuex-typing.git"
  },
  "bugs": {
    "url": "https://github.com/d-kimuson/vuex-typing/issues"
  },
  "scripts": {
    "build": "tsc -p .",
    "dev": "tsc -p . --watch",
    "typecheck": "tsc -p . --noEmit",
    "typecheck:example": "tsc -p example --noEmit",
    "lint": "yarn typecheck && run-p lint:*",
    "lint:eslint": "eslint src --ext .ts",
    "lint:prettier": "prettier '**/*.{ts,json,yml,yaml}' --check",
    "fix": "run-p fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "prettier '**/*.{ts,json,yml,yaml}' --write",
    "lint-staged": "lint-staged --allow-empty",
    "prepare": "husky install",
    "prepublish": "yarn build",
    "test": "jest"
  },
  "lint-staged": {
    "./**/*.ts": [
      "eslint --fix"
    ],
    "./**/*.{ts,json,yml,yaml}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "vue": "3",
    "vuex": "4"
  },
  "devDependencies": {
    "@babel/types": "^7.15.6",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "tsd": "^0.17.0",
    "typescript": "^4.4.3",
    "vue": "3",
    "vuex": "4"
  },
  "license": "MIT",
  "keywords": [
    "vuex",
    "typescript"
  ]
}
